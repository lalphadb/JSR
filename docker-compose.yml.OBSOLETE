services:
  frontend:
    build: .
    container_name: jsr-dev
    restart: unless-stopped
    networks:
      - traefik-net
    labels:
      - "traefik.enable=true"
      - "traefik.http.routers.jsr-dev.rule=Host(`jsr.4lb.ca`)"
      - "traefik.http.routers.jsr-dev.entrypoints=websecure"
      - "traefik.http.routers.jsr-dev.tls.certresolver=letsencrypt"
      - "traefik.http.services.jsr-dev.loadbalancer.server.port=80"

networks:
  traefik-net:
    external: true
